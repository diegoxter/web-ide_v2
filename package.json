		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-svelte-plugin": "^0.3.42",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"@sveltestrap/sveltestrap": "^6.2.7",
		"ace-builds": "^1.36.3"
	}
